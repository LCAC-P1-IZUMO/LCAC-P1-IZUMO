<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <TextView
        android:id="@+id/lavel_MEMBER"
        android:layout_width="166dp"
        android:layout_height="wrap_content"
        android:layout_x="19dp"
        android:layout_y="19dp"
        android:text="■メンバ一覧"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <ProgressBar
        android:id="@+id/progressBar1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="268dp"
        android:layout_y="-50dp" />

    <Button
        android:id="@+id/button_BACK"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_x="10dp"
        android:layout_y="407dp"
        android:text="戻る" />

    <Button
        android:id="@+id/Button_NEXT"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_x="203dp"
        android:layout_y="407dp"
        android:text="次へ" />

    <Button
        android:id="@+id/Button_SAVE"
        android:layout_width="78dp"
        android:layout_height="wrap_content"
        android:layout_x="124dp"
        android:layout_y="407dp"
        android:text="保存" />

    <EditText
        android:id="@+id/editText1"
        android:layout_width="185dp"
        android:layout_height="wrap_content"
        android:layout_x="19dp"
        android:layout_y="348dp"
        android:ems="10"
        android:inputType="textPersonName" >

        <requestFocus />
    </EditText>

    <Button
        android:id="@+id/button_ADD"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:layout_x="213dp"
        android:layout_y="341dp"
        android:text="追加" />

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="296dp"
        android:layout_height="266dp"
        android:layout_x="8dp"
        android:layout_y="66dp" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >
        </LinearLayout>
    </ScrollView>

    <Button
        android:id="@+id/Button_GRPGET"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="177dp"
        android:layout_y="9dp"
        android:text="グループ呼出" />

</AbsoluteLayout>
